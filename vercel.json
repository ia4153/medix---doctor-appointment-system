{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "^/(?!api).*",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE": {
      "description": "API base URL for the backend"
    }
  }
}
